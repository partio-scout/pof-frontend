{
    "name": "partio-scout/pof-frontend-private",
    "config": {
        "preferred-install": "dist",
        "discard-changes": false,
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Ville Siltala",
            "email": "ville.siltala@geniem.com",
            "homepage": "https://github.com/villesiltala",
            "role": "Project Lead Developer"
        }
    ],
    "require": {
        
        "johnpbloch/wordpress-core-installer": "^1.0.0",
        "johnpbloch/wordpress": "^4.7",
        "devgeniem/dustpress": "^1.6.9",
        "devgeniem/dustpress-debugger": "1.*",
        "devgeniem/dustpress-js": "^2.0.0",
        "devgeniem/ext-relevanssi-premium": "^1.14.9.1",
        "partio-scout/wordpress-advanced-custom-fields-pro": "^5.3",
        "vlucas/phpdotenv": "^2.4",
        "composer/installers": "^1.3",
        "predis/predis": "^1.1",
        "wpackagist-plugin/user-switching": "^1.0",
        "devgeniem/wp-redis-object-cache-dropin": "1.3.4.1",
        "wpackagist-plugin/redis-cache": "1.3.0"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "dropin-paths": {
            "content/": [
                "type:wordpress-dropin"
            ]
        }
    },
    "repositories": {
        "1": {
            "type": "git",
            "url": "git@github.com:partio-scout/wordpress-advanced-custom-fields-pro.git"
        },
        "2": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "relevanssi" : {
            "type": "git",
            "url": "git@github.com:devgeniem/ext-relevanssi-premium.git"
        },
        "devgeniem/wp-redis-object-cache-dropin": {
            "type": "git",
            "url": "git@github.com:devgeniem/wp-redis-object-cache-dropin.git"
        }      
    }
}
